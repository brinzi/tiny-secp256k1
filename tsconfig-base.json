{
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "module": "esnext",
    "target": "es2017",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noPropertyAccessFromIndexSignature": true,
    "moduleResolution": "node",
    "baseUrl": "src_ts"
  },
  "include": ["src_ts/*.ts"]
}
